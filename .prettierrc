{
    "arrowParens": "always",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "checkIgnorePragma": true,
    "cursorOffset": -1,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "objectWrap": "preserve",
    "printWidth": 200,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "requirePragma": false,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "singleAttributePerLine": false,
    "useTabs": false,
    "plugins": ["prettier-plugin-tailwindcss"],
    "overrides": [
        {
            "files": ["*.yaml", "*.yml"],
            "options": {
                "tabWidth": 2
            }
        }
    ]
}
